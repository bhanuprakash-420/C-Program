#include <stdio.h>

int main()
{
 int a[2][2]={10,8,7,12};
 int b[2][2]={8,2,5,6};
 int sub [2][2]={0,0,0,0};
 int i,j;                                                                                                                                                
 for(i=0;i<2;i++)
 
      {
          for(j=0;j<2;j++)
            {
                sub[i][j]=a[i][j]-b[i][j];
            }
      }
      for(i=0;i<2;i++)
      
      { for(j=0;j<2;j++)
      { printf("%d\t",sub[i][j]);
          
      }printf("\n");
      }
      
    return 0;
}
